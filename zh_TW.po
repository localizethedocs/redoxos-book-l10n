msgstr ""

#: src/boot-process.md:43
msgid "`/scheme/debug`"
msgstr ""

#: src/boot-process.md:44
msgid "`/scheme/irq`"
msgstr ""

#: src/boot-process.md:45
msgid "`/scheme/serio`"
msgstr ""

#: src/boot-process.md:47
msgid ""
